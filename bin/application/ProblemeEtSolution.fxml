<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane style="-fx-background-color: #4B535E;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TableDeProblemesController">
   <center>
      <TableView fx:id="tableDeProblemes" prefHeight="400.0" prefWidth="700.0" stylesheets="@../styleFile/tableViewChronoGramme.css">
        <columns>
          <TableColumn fx:id="type" prefWidth="118.0" text="TYPE" />
          <TableColumn fx:id="probleme" prefWidth="291.0" text="PROBLEME" />
            <TableColumn fx:id="solution" prefWidth="290.0" text="SOLUTION" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <Button fx:id="documentation" mnemonicParsing="false" onAction="#onDocumente" prefHeight="25.0" prefWidth="169.0" text="Documentation" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" top="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
</BorderPane>
